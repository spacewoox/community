<div class="section">
  <h4>
    History 
  </h4>
  <p class="color-primary indication">History gives you information regarding your user's sessions
  You can also download this table as CSV file
    <p class="m-t-1 m-b-0"> 
      <button class='btn btn-primary downloadcsv' {{ action 'downloadCSV' }}>Save as SCV file</button>
    </p>
  </p>
  <div class='content-wrapper'>

    {{#if model}}
      <table class="table">
        <thead>
          <tr>
            <th>Id user</th>
            <th>Connection</th>
            <th>Start</th>
            <th>End</th>
          </tr>
        </thead>
        <tbody>
            {{#each model as |history|}}
              <tr>
                <td>{{history.userId}}</td>
                <td>{{history.connectionId}}</td>
                <td>{{history.startDate}}</td>
                <td>{{history.endDate}}</td>
              </tr>
            {{/each}}
        </tbody>
      </table>
    {{else}}
      No data available
    {{/if}}
    <div class="clearfix"></div>
  </div>

</div>
